        <ScrollViewer  Height="343" Margin="81,89,71.6,0" CanContentScroll="True" VerticalAlignment="Top">
            <StackPanel x:Name="Stack" Height="336" Width="619"/>
        </ScrollViewer>
        <Button Content="Отмена" HorizontalAlignment="Left" Margin="684,41,0,0" VerticalAlignment="Top" Width="75" Height="30" Click="Button_Click" Background="#FF95A4AC" FontFamily="Sylfaen" FontSize="14"/>
        <Label Content="Поиск сообщений" HorizontalAlignment="Left" Margin="294,0,0,0" VerticalAlignment="Top" Height="41" Width="214" FontFamily="Sylfaen" FontSize="24" FontWeight="Bold" Foreground="#FF0D0D0E"/>

    </Grid>
</Window>
