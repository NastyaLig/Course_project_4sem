        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseProject"
        mc:Ignorable="d"
        Title="Search" Height="471.913" Width="800" Background="#FF215B7A">
    <Grid>
        <TextBox x:Name="text" HorizontalAlignment="Left" Height="30" Margin="81,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="510" Background="#FF0F3753" BorderBrush="#FF0F3753" Foreground="#FFABBEC8" FontFamily="Sylfaen" FontSize="14"/>
